{
	"name": "zenify/doctrine-behaviors",
	"description": "Integration of KnpLabs/DoctrineBehaviors to Nette\\DI",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/PavelJurasek/events"
		}
	],
	"require": {
		"php": "^7.1",
		"kdyby/events": "^4.0",
		"doctrine/orm": "^2.6",
		"knplabs/doctrine-behaviors": "^1.6",
		"tracy/tracy": "^2.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0",
		"nette/bootstrap": "^3.0",
		"nette/di": "^3.0",
		"nette/http": "^3.0",
		"nette/reflection": "^2.4",
		"nette/security": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineBehaviors\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineBehaviors\\Tests\\": "tests"
		}
	}
}
